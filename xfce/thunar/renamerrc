[Configuration]
LastActiveMode=THUNAR_RENAMER_MODE_NAME
LastActiveRenamer=ThunarSbrRemoveRenamer

[ThunarSbrInsertRenamer]
Offset=20
Mode=THUNAR_SBR_INSERT_MODE_INSERT
OffsetMode=THUNAR_SBR_OFFSET_MODE_RIGHT
Text=Screenshot

[ThunarSbrReplaceRenamer]
Regexp=TRUE
Replacement=-
CaseSensitive=FALSE
Pattern=[0-9]_

[ThunarSbrRemoveRenamer]
EndOffsetMode=THUNAR_SBR_OFFSET_MODE_LEFT
StartOffset=0
EndOffset=25
StartOffsetMode=THUNAR_SBR_OFFSET_MODE_LEFT

