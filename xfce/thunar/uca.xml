<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Terminal ici</name>
	<unique-id>1504349608344638-1</unique-id>
	<command>exo-open --working-directory %f --launch TerminalEmulator</command>
	<description>Exemple d&apos;une action personnalisée</description>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
	<icon>vlc</icon>
	<name>VLC</name>
	<unique-id>1505849651010786-2</unique-id>
	<command>vlc --overlay %F</command>
	<description></description>
	<patterns>*.part;*</patterns>
	<directories/>
	<audio-files/>
	<other-files/>
	<video-files/>
</action>
<action>
	<icon>application-x-compress</icon>
	<name>Extraire ici (zip)</name>
	<unique-id>1505384164362410-5</unique-id>
	<command>extract %f</command>
	<description></description>
	<patterns>*.zip</patterns>
	<other-files/>
</action>
<action>
	<icon>system-file-manager</icon>
	<name>Ouvrir en mode root</name>
	<unique-id>1505384439089182-7</unique-id>
	<command>pkexec thunar %f</command>
	<description></description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon></icon>
	<name>JPEG Optim</name>
	<unique-id>1542619832610200-1</unique-id>
	<command>for file in %F; do jpegoptim --strip-all -of &quot;$file&quot;; done</command>
	<description>Losslessly optimize JPEGs, by optimizing the Huffman tables and stripping comments and EXIF metadata from the file.</description>
	<patterns>*.jpg;*.jpeg</patterns>
	<image-files/>
</action>
<action>
	<icon>clipboard</icon>
	<name>Copier le chémin absolu</name>
	<unique-id>1505388833482118-19</unique-id>
	<command>realpath %f | xclip -in -selection clipboard</command>
	<description></description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
</actions>
