{
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.formatOnPaste": true,
  "workbench.startupEditor": "none",
  "workbench.iconTheme": "eq-material-theme-icons-palenight",
  "workbench.colorTheme": "Material Theme Palenight",
  "workbench.sideBar.location": "left",
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.fontFamily": " Dank Mono , Fira Code , Roboto Medium",
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.tabSize": 2,
  "editor.renderWhitespace": "boundary",
  "editor.tabCompletion": true,
  "editor.fontWeight": "800",
  "editor.letterSpacing": "0.5",
  "editor.lineNumbers": "relative",
  "editor.renderLineHighlight": "all",
  "editor.minimap.enabled": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
  "debug.allowBreakpointsEverywhere": true,
  "zenMode.hideTabs": false,
  "zenMode.hideStatusBar": false,
  "terminal.integrated.fontSize": 16,
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": false,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "editor.insertSpaces": true,
  "color-highlight.markerType": "underline",
  "window.zoomLevel": 0,
  "vim.searchHighlightColor": "rgb(71, 59 108)",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-d>": false,
    "<C-x>": false,
    "<C-y>": false,
    "<C-f>": false,
    "<C-c>": false
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["z", "z"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.files.save",
          "args": []
        },
        {
          "command": "workbench.action.closeActiveEditor",
          "args": []
        }
      ]
    },
    {
      "before": ["<C-y>"],
      "after": ["4", "<C-y>"]
    },
    {
      "before": ["<C-e>"],
      "after": ["4", "<C-e>"]
    },
    {
      "before": ["Y"],
      "after": ["y", "$"]
    },
    {
      "before": ["<leader>", "o"],
      "after": ["o", "<Esc>"]
    },
    {
      "before": ["<leader>", "O"],
      "after": ["O", "<Esc>"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    },
    {
      "before": ["z", "z"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.files.save",
          "args": []
        },
        {
          "command": "workbench.action.closeActiveEditor",
          "args": []
        }
      ]
    }
  ],
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.leader": ",",
  "vim.enableNeovim": true,
  "vim.cmdLineInitialColon": true,
  "go.gopath": "~/.offline/golang",
  "eslint.nodePath": "~/.lib/npm/bin/eslint",
  "extensions.ignoreRecommendations": true,
  "newFile.defaultBaseFileName": "newFile",
  "newFile.relativeTo": "project", // "root" or "project"
  "newFile.defaultFileExtension": ".js",
  "newFile.rootDirectory": "~",
  "newFile.showPathRelativeTo": "project", // "project" or "none"
  "newFile.expandBraces": true, // setting to true will allow for creating multiple files such as `new-folder/{file1,file2}.js`
  "editor.detectIndentation": true,
  "java.home": "/usr/lib/jvm/java-8-openjdk",
  "gitlens.gitExplorer.files.layout": "auto",
  "cSpell.language": "en,fr",
  "vim.disableExtension": false,
  "liveServer.settings.donotShowInfoMsg": true,

  "git.enableSmartCommit": true,
  "java.errors.incompleteClasspath.severity": "ignore",
  "editor.selectionHighlight": false,
  "indentRainbow.colors": [
    "rgba(61,64,16,0.2)",
    "rgba(32,64,32,0.2)",
    "rgba(64,32,64,0.2)",
    "rgba(16,48,48,0.2)"
  ],
  "indenticator.width": 1,
  "editor.wordWrap": "bounded",
  "editor.mouseWheelZoom": true,

  "editor.minimap.renderCharacters": false,
  "editor.formatOnSave": true,
  "html.format.enable": true,
  "html.format.endWithNewline": true,
  "html.format.preserveNewLines": true,
  "prettier.eslintIntegration": true,
  "prettier.stylelintIntegration": true,
  "eslint.autoFixOnSave": false,
  "eslint.alwaysShowStatus": true,
  "diffEditor.ignoreTrimWhitespace": true,
  "files.exclude": {
    "**/.git": true,
    "nodes_modules": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "*.class": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "files.associations": {
    "*.js": "javascriptreact"
  },
  "files.trimTrailingWhitespace": true,

  "scm.alwaysShowProviders": true,
  "vim.neovimPath": "/usr/bin/nvim",
  "emmet.triggerExpansionOnTab": false,

  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "mjml": "html"
  },
  "window.menuBarVisibility": "visible",

  "breadcrumbs.enabled": true,
  "git.autorefresh": false,
  "git.alwaysSignOff": true,
  "git.confirmSync": false,
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#b52e31"
    },
    {
      "name": "Auth0 Orange",
      "value": "#eb5424"
    },
    {
